<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
        <Configuration>
          <Format>cobertura</Format>
          <Include></Include>
          <Exclude>
            <!-- [Assembly Specification]Namespace Specification* -->
            [Business.Infrastructure]Business.Infrastructure.Migrations*
          </Exclude>
          <SingleHit>true</SingleHit>
          <SkipAutoProps>true</SkipAutoProps>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>